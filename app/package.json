{
  "name": "doctorino",
  "version": "1.0.0",
  "description": "Doctorino - Medical Practice Management System with ECG analysis capabilities",
  "main": "main.js",
  "scripts": {
    "start": "python main.py",
    "dev": "python main.py --debug",
    "browser": "python main.py --browser",
    "build": "cd frontend && npm run build && cd .. && python -m PyInstaller --onefile --windowed main.py",
    "setup-venv": "python -m venv venv && source venv/bin/activate && pip install -r backend/requirements.txt",
    "postinstall": "cd frontend && npm install"
  },
  "keywords": [
    "electron",
    "python",
    "fastapi",
    "react",
    "typescript",
    "healthcare",
    "patient-management",
    "ecg-analysis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^7.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.15.17",
    "@vitejs/plugin-react": "^4.4.1",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "electron": "^36.2.0",
    "electron-builder": "^26.0.12",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.6.0",
    "tailwindcss": "^4.1.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
